<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:rpcConsumer="http://com.rpc.rpcdemo/beandefinition/consumer"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://com.rpc.rpcdemo/beandefinition/consumer http://com.rpc.rpcdemo/beandefinition/consumer-defination.xsd">

    <rpcConsumer:socketCustom zkIp="127.0.0.1" zkPort="2181" zkTimeout="5000" consumerPort="20880"/>

    <rpcConsumer:rpcService serviceClassName="com.rpc.rpcdemo.service.RpcDemoGoodsService" ref="rpcDemoGoodsService"/>
    <rpcConsumer:rpcService serviceClassName="com.rpc.rpcdemo.service.RpcDemoEmployeService" ref="rpcDemoEmployeService"/>

</beans>